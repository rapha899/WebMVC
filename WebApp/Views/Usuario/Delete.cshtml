@using Clases
@model Usuario

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Borrar Usuario</h1>

<div class="container">
 <div class="row">
<div class="col-sm-12">
</div>
</div>
<div class="row">
    <div class="col-sm-10">
        <form method="post" asp-action="Delete">
            <div class="form-group">
                <label class="custom-control-label">Nombre</label>
                <input class="form-control" asp-for="Nombre" disabled/>
            </div>
             <div class="form-group">
                <label class="custom-control-label">Apellido</label>
                <input class="form-control" asp-for="Apellido" disabled/>
            </div>
             <div class="form-group">
                <label class="custom-control-label">Cedula</label>
                <input class="form-control" asp-for="Cedula" disabled/>
            </div>
             <div class="form-group">
                <label class="custom-control-label">Correo</label>
                <input class="form-control" asp-for="Email" disabled/>
            </div>
             <div class="form-group">
                <label class="custom-control-label">Edad</label>
                <input class="form-control" asp-for="Edad" disabled/>
            </div>
            <div class="form-group">
                <input class="btn btn-primary" type="submit" value="Confirmar borrado de Usuario" />
            </div>
        </form>
</div>
</div>

</div>